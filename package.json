    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-query": "3.39.3",
    "react-router-dom": "6.8.0"
  },
  "devDependencies": {
    "@types/node": "16.7.13",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.0",
