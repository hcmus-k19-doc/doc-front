{
  "name": "doc-front",
  "homepage": "/doc",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/compatible": "^5.1.1",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-components": "2.3.52",
    "@ckeditor/ckeditor5-build-classic": "^37.1.0",
    "@ckeditor/ckeditor5-react": "^6.0.0",
    "@cyntler/react-doc-viewer": "^1.13.0",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "4.26.1",
    "antd": "5.1.6",
    "axios": "1.2.6",
    "date-fns": "2.29.3",
    "dayjs": "^1.11.7",
    "echarts": "5.4.2",
    "echarts-for-react": "3.0.2",
    "file-saver": "^2.0.5",
    "i18next": "22.4.11",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-http-backend": "2.2.1",
    "moment": "^2.29.4",
    "qs": "6.11.1",
    "rc-virtual-list": "^3.4.13",
    "react": "18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-router-dom": "6.8.0",
    "react-scripts": "5.0.1",
    "react-topbar-progress-indicator": "^4.1.1",
    "recoil": "0.7.7",
    "sweetalert2": "^11.7.3",
    "sweetalert2-react-content": "^5.0.7",
    "tailwindcss": "3.2.4"
  },
  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:release": "env-cmd -f .env.release react-scripts build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint-staged": "lint-staged",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc.json",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "postinstall": "npx husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/node": "16.7.13",
    "@types/react": "18.0.0",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lint-staged": "13.1.2",
    "prettier": "2.8.3",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run format",
      "git add ."
    ]
  }
}
